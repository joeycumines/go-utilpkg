#!/bin/bash
# Comprehensive cleanup of all temporary/debugging files

set -e
cd /Users/joeyc/dev/go-utilpkg

echo "=== Comprehensive cleanup of temporary files ==="

# Remove temporary scripts and makefiles
rm -f cleanup_temp_test_files.sh
rm -f custom_test.mk

# Remove temporary eventloop files
rm -f eventloop/Makefile
rm -f eventloop/config.mk
rm -f eventloop/coverage_gap_tests.go

# Remove temporary test files from eventloop/
rm -f eventloop/internal/promisealtthree/promise_test.go
rm -f eventloop/internal/promisealttwo/promise_test.go
rm -f eventloop/loop_edge_cases_test.go
rm -f eventloop/metrics_coverage_test.go
rm -f eventloop/poller_darwin_coverage_test.go
rm -f eventloop/promise_all_concurrency_test.go
rm -f eventloop/promise_allsettled_concurrency_test.go
rm -f eventloop/promise_any_concurrency_test.go
rm -f eventloop/promise_catch_finally_test.go
rm -f eventloop/promise_edge_cases_test.go
rm -f eventloop/promise_race_concurrency_test.go
rm -f eventloop/promise_tochannel_test.go
rm -f eventloop/tochannel_test.go

# Remove coverage analysis docs (regenerated on demand)
rm -f eventloop/COVERAGE_ANALYSIS_FULL.md
rm -f eventloop/COVERAGE_GAP_ANALYSIS.md

# Remove session documentation (regenerated)
rm -f .session_time
rm -f SESSION_PROGRESS.md

# Remove old cleanup script (covered by this script)
rm -f cleanup_temp_test_files.sh 2>/dev/null || true

echo "=== Comprehensive cleanup complete ==="
